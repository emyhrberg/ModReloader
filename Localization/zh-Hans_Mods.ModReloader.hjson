Configs: {
	Config: {
		DisplayName: 配置

		Headers: {
			Improvements: 改进
			Misc: 杂项
			ExtraInfo: 显示
			ModsToReload: 重加载时需要构建的模组
			ReloadOptions: 重加载选项
		}

