Configs: {
	Config: {
		DisplayName: Config

		Headers: {
			Improvements: Improvements
			Misc: Misc
			ExtraInfo: Show
			ModsToReload: Mods To Reload
			ReloadOptions: Reload Options
		}

		ModsToReload: {
			Label: Mods To Reload
			Tooltip: ""
		}

		AutoJoinWorld: {
			Label: Auto Join World
			Tooltip: Automatically joins the selected world after reloading mods
		}

		Player: {
			Label: Player
			Tooltip: Select the player to join a world with when reloading
		}

		World: {
			Label: World
			Tooltip: Select the world to join when reloading
		}

		LogDebugMessages: {
			Label: Log Debug Messages
			Tooltip:
				'''
				(Optional) Logs the flow of actions in the mod. 
				Useful for finding the cause of bugs.
				'''
		}

		ShowDebugInfo: {
			Label: Show Debug Info
			Tooltip:
				'''
				Adds a debug text in the bottom left corner when in a world
				Contains name, ID, mode, fps, open config.
				'''
		}

		ShowMainMenuInfo: {
			Label: Show Main Menu Info
			Tooltip: Adds all of the main menu options to that screen
		}

		ShowErrorMenuInfo: {
			Label: Show Error Menu Info
			Tooltip: Adds all of the error menu options to that screen
		}

		ShowCopyToClipboardButton: {
			Label: Show Copy To Clipboard Button
			Tooltip: A new button will appear allowing you to copy the message in the error menu
		}

		RightClickToolOptions: {
			Label: Right Click Tool Options
			Tooltip: Adds the option for tools to have additional functionality with right click options.
		}

		MoveChat: {
			Label: Move Chat
			Tooltip: Moves the chat box position a little to make space for debug info
		}

		LogLevelPersistOnReloads: {
			Tooltip: Whether to keep the log levels after reloading. Keep this setting off to keep log levels at default.
			Label: Log Level Persist On Reloads
		}

		BuilderToggle: {
			Label: Builder Toggle
			Tooltip: Shows a new builder toggle to the left of inventory to toggle all tools
		}

		ShowBuilderToggle: {
			Label: Show Builder Toggle
			Tooltip: Shows a toggle to hide or show everything in the top left of the inventory
		}

		ClearLogOnReload: {
			Label: Clear Log On Reload
			Tooltip: Enable this to clear the client log every time a reload is performed
		}

		SaveWorld: {
			Label: Save World
			Tooltip:
				'''
				Save player and world data before reloading. 
				Takes more time to reload but saves the your player and world
				'''
		}

		CreateTestWorldSize: {
			Label: Create Test World Size
			Tooltip: Size set to a world created from the main menu option
		}

		CreateTestWorldDifficulty: {
			Label: Create Test World Difficulty
			Tooltip: Difficulty set to a world created from the main menu option
		}
	}

	WorldSize: {
		Tooltip: ""
		Small.Label: Small
		Medium.Label: Medium
		Large.Label: Large
		ExtraSmall.Label: Extra Small (Risky)
	}

	WorldDifficulty: {
		Tooltip: ""
		Normal.Label: Normal
		Expert.Label: Expert
		Master.Label: Master
		Journey.Label: Journey
	}
}

